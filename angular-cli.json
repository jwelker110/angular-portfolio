{
  "project": {
    "name": "portfolio"
  },
  "apps": [
    {
      "root": "src",
      "assets": [
        "assets/**",
        "favicon.ico"
      ],
      "baseHref": "/",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "ap",
      "mobile": false,
      "styles": [
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/marked/marked.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.dev.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "serve": {},
  "defaults": {
    "styleExt": "scss",
    "lintFix": true,
    "class": {
      "spec": false
    },
    "component": {
      "spec": false
    },
    "directive": {
      "spec": false
    },
    "pipe": {
      "spec": false
    },
    "service": {
      "spec": false
    },
    "interface": {
      "prefix": "I"
    }
  }
}
